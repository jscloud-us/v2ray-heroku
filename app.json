{
    "name": "V2Ray",
    "description": "Deploy V2ray on Heroku.",
    "keywords": ["V2Ray"],
    "env": {
        "UUID": {
            "description": "V2Ray UUID",
            "value": "$(cat /proc/sys/kernel/random/uuid)"
        },
        "WSPATH": {
            "description": "V2Ray WSPATH",
            "value": "$(head /dev/urandom | tr -dc A-Za-z0-9 | head -c 16)"
        }
    },
    "website": "https://ibcl.us/Heroku-V2Ray_20191014/",
    "repository": "https://github.com/bclswl0827/v2ray-heroku",
    "stack": "container"
}
